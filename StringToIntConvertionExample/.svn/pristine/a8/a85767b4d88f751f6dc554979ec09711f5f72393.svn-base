public class String_Int_StringApp {

    /**
     * input : String || output : void || function : Convert the given String to int
     * type and print it
     * 
     * @param num
     */
    public static void convertStringToInt(String num) {
	System.out.println("Number after conversion using Integer.parseInt(num) : " + Integer.parseInt(num));
	System.out.println("Number after conversion using Integer.valueOf(num) : " + Integer.valueOf(num));
    }// method

    /**
     * input : int || output : void || function : Convert the given int to String
     * type and print it
     * 
     * @param num
     */
    public static void convertIntToString(int num) {
	String number = num + "";
	System.out.println("Number after conversion using +[concatination] : " + number);
	System.out.println("Number after conversion using String.valueOf(num) : " + String.valueOf(num));
    }// method

}// class
